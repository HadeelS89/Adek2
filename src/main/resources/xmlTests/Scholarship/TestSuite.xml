<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">


<suite name=" Sholarship Test">

    <listeners>
        <listener class-name="com.qpros.reporting.Listeners" />
    </listeners>


    <test name="Authorization Test" thread-count="10">
      <classes>
          <class name="com.qpros.authorization.LoginTest">
              <methods>
                  <include name="Looog"></include>
              </methods>
          </class>
          <class name="com.qpros.Test">
              <methods>
                  <include name="setProgramTeam"></include>
                  <include name="submitProgram"></include>
                  <include name="searchApplicationsAndProcessThem"></include>
              </methods>
          </class>
      </classes>
    </test>
</suite>